<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youth.dao.UserDao">
    <select id="getUserByName" parameterType="java.lang.String" resultType="com.youth.bean.User">
        SELECT * FROM users WHERE  name = #{name}
    </select>
</mapper>